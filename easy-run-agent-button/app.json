{
    "name": "test-app",
    "options": {
        "allow-unauthenticated": true,
        "memory": "512Mi",
        "cpu": "1",
        "port": 80,
        "http2": false
    },
    "hooks": {
        "postcreate": {
            "commands": [
                "gcloud run services delete test-app --region $GOOGLE_CLOUD_REGION --project $GOOGLE_CLOUD_PROJECT --quiet",
                "echo 'Your Dialogflow Agent is Deployed to $AGENT_URL'"
            ]
        }
    }
}